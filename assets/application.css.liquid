.marquee-wrap {
  position: relative;
  width: 100%;
  height: 80px;
  overflow: hidden;
  background: #F8FDF0;
  border-top: 1px solid #aeadf2;
  border-bottom: 1px solid #aeadf2;
}
.marquee-wrap .marquee-inner {
  width: 100%;
  position: absolute;
  top: 50%;
  left: -20px;
  transform: translate(0, -50%);
  white-space: nowrap;
  transition: 0.1s ease-out;
  animation: marquee 40s linear infinite;
  animation-delay: 0.3s;
  padding: 30px 0;
}
.marquee-wrap .marquee-inner:hover {
  animation-play-state: paused;
}

@keyframes marquee {
  0% {
    left: -20%;
  }
  50% {
    left: -100%;
  }
  100% {
    left: -20%;
  }
}
.masonary {
